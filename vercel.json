{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/crypto",
      "dest": "/api/crypto.js"
    },
    {
      "src": "/news",
      "dest": "/api/news.js"
    },
    {
      "src": "/videos",
      "dest": "/api/videos.js"
    }
  ]
}